{
	"source": {
		"includePattern": ".+\\.js?$",
		"include": [".", "./README.md"],
		"exclude": ["node_modules"]
	},
	"recurseDepth": 10,
	"opts": {
		"destination": "./docs/",
		"recurse": true,
		"template": "./node_modules/docdash"
	},
	"templates": {
		"default": {
			"includeDate": false
		}
	},
	"docdash": {
		"openGraph": {
			"title": "AniList-Node Documentation",
			"type": "website",
			"site_name": "AniList-Node Documentation",
			"url": "https://katsurin.com/docs/anilist-node/"
		},
		"meta": {
			"title": "AniList-Node Documentation",
			"description": "Documentation for the AniList-Node NPM package."
		},
		"sort": true,
		"search": true,
		"typedefs": true,
		"menu": {
			"GitHub Repository": {
				"href": "https://github.com/AurelicButter/AniList-Node",
				"target": "_blank",
				"class": "menu-item",
				"id": "website_link"
			},
			"AniList-Node on NPM": {
				"href": "https://www.npmjs.com/package/anilist-node",
				"target": "_blank",
				"class": "menu-item",
				"id": "website_link"
			},
			"Support Server": {
				"href": "https://discord.gg/qKfqsjW",
				"target": "_blank",
				"class": "menu-item",
				"id": "website_link"
			}
		}
	}
}
