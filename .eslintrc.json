{
	"env": {
		"commonjs": true,
		"es6": true,
		"node": true
	},
	"extends": "eslint:recommended",
	"parserOptions": {
		"ecmaVersion": 12
	},
	"rules": {
		"no-extra-parens": "error",
		"eqeqeq": "warn",
		"no-else-return": "error",
		"camelcase": "warn",
		"comma-dangle": "warn",
		"no-var": "error"
	}
}
